{
	"name": "@dumpstate/web-worker-proxy",
	"version": "0.2.0",
	"description": "Web worker proxy for JavaScript objects.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsc",
		"build:test:worker": "esbuild test/workers/foo.worker.ts --bundle --outfile=build/foo.worker.js --format=iife",
		"lint": "prettier --check .",
		"prepublishOnly": "npm run build",
		"style:fix": "prettier --write .",
		"test": "npm run build:test:worker && vitest run"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dumpstate/web-worker-proxy.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"author": "asadowski@mailbox.org",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dumpstate/web-worker-proxy/issues"
	},
	"homepage": "https://github.com/dumpstate/web-worker-proxy#readme",
	"devDependencies": {
		"@vitest/browser": "^3.0.5",
		"esbuild": "^0.25.0",
		"playwright": "^1.50.1",
		"prettier": "^3.8.1",
		"typescript": "^5.9.3",
		"vitest": "^3.0.5"
	}
}
